    "/prj/ctPrj": {
      "post": {
        "tags": [
          "prj"
        ],
        "summary": "프로젝트 생성",
        "description": "사용자가 프로젝트를 생성한다.",
        "consumes": ["multipart/form-data"],
        "parameters": [
          {
            "name": "prj_name",
            "in": "formData",
            "type": "string",
            "required": true,
            "example": "프로젝트1",
            "description" : "프로젝트 이름"
          },
          {
            "name": "prj_description",
            "in": "formData",
            "type": "string",
            "required": true,
            "example": "이 프로젝트는 러시아에서 시작되어 전세계적으로 펼쳐져..",
            "description" : "프로젝트 설명"
          },
          {
            "name": "prj_start_version",
            "in": "formData",
            "type": "string",
            "required": true,
            "example": "v0.0.1",
            "description" : "프로젝트 버전"
          },
          {
            "name": "rgst_user_id",
            "in": "formData",
            "type": "number",
            "required": true,
            "example": 1,
            "description" : "프로젝트 생성 유저 아이디"
          },
          {
            "name": "prj_lnk",
            "in": "formData",
            "type": "string", 
            "example": "http://example.com",
            "required" : true,
            "description" : "프로젝트 링크"
          },
          {
            "name": "prj_sec_user",
            "in": "formData",
            "type": "array",
            "items" : {
              "type" : "number"
            },
            "required": true,
            "example": 2,
            "description" : "프로젝트 보안 담당자(다중)"
          },
          {
            "name": "prj_dev_user",
            "in": "formData",
            "type": "array",
            "items" : {
              "type" : "number"
            },
            "required": true,
            "example": 1,
            "description" : "프로젝트 개발 담당자(다중)"
          },
          {
            "name": "files",
            "in": "formData",
            "type": "array",
            "items" : {
              "type" : "file"
            },
            "required": true,
            "description" : "프로젝트 파일(다중)"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "object",
              "properties": {
                "resultCode": {
                  "type": "number",
                  "example": 200
                },
                "resultMsg": {
                  "type": "string",
                  "example": "프로젝트 생성 완료"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
